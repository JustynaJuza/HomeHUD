{
  "Logging": {
    "IncludeScopes": false,
    "LogLevel": {
      "Default": "Debug",
      "Microsoft": "Warning",
      "Microsoft.EntityFrameworkCore": "Information",
      "System": "Information"
    },
    "FormatPattern": "| %utcdate{dd/MM/yyyy HH:mm:ss} (%date{dd/MM/yyyy HH:mm:ss}) | %timestamp | %8level | [%thread: %logger%] %message%newline"
  },
  "showDetailedErrors": ""
}
